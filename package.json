  "scripts": {
    "client-test": "NODE_ENV=test ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha ./client/utils/config.js -R spec ./client/tests/**",
    "migrate": "sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed:all",
    "start": "node index.js",
    "server-test": "NODE_ENV=test npm run migrate && ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -R spec server/tests",
    "test-migrate": "sequelize db:migrate:undo:all && sequelize db:migrate",
    "test": "npm run server-test && npm run client-test",
    "coverage": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
